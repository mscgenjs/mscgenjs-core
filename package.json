{
  "name": "mscgenjs",
  "version": "1.15.2",
  "description": "Sequence chart rendering library",
  "main": "index.js",
  "dependencies": {
    "amdefine": "1.0.1",
    "requirejs": "2.3.5"
  },
  "devDependencies": {
    "almond": "0.3.3",
    "chai": "4.1.2",
    "chai-xml": "0.3.2",
    "dependency-cruiser": "4.0.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-security": "1.4.0",
    "jest": "22.4.4",
    "js-makedepend": "3.0.0",
    "jsdom": "11.10.0",
    "lodash-cli": "4.17.5",
    "npm-check-updates": "2.14.2",
    "nsp": "3.2.1",
    "pegjs": "0.10.0"
  },
  "scripts": {
    "depcruise": "depcruise --validate -- main parse render *.js",
    "depcruise:graph": "depcruise --validate --output-type dot --module-systems amd --exclude \"amdefine|lodash\" main parse render *.js | dot -T svg > tmp_deps.svg",
    "lint": "eslint index.js index-lazy.js indexAMD.js main parse render test",
    "lint:fix": "eslint --fix index.js index-lazy.js indexAMD.js main parse render test",
    "lint:types": "tslint types/mscgen.d.ts",
    "npm-check-updates": "ncu --upgrade",
    "nsp": "nsp check",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mscgenjs/mscgenjs-core"
  },
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "keywords": [
    "mscgen",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "index.js",
      "index-lazy.js",
      "main/**/*.js",
      "parse/**/*.js",
      "render/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "render/graphics/styling",
      "render/graphics/svgelementfactory/wobbly\\.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 99.8,
        "branches": 92.9,
        "functions": 100,
        "lines": 99.8
      },
      "main": {
        "statements": 100,
        "branches": 93.9,
        "functions": 100,
        "lines": 100
      },
      "render": {
        "statements": 99.8,
        "branches": 92.8,
        "functions": 100,
        "lines": 99.8
      },
      "parse": {
        "statements": 83,
        "branches": 74,
        "functions": 91,
        "lines": 87
      }
    }
  },
  "engines": {
    "node": ">=4.0"
  },
  "types": "./types/mscgen.d.ts",
  "bugs": {
    "url": "https://github.com/mscgenjs/mscgenjs-core/issues"
  },
  "homepage": "https://github.com/mscgenjs/mscgenjs-core"
}
